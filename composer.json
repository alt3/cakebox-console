{
	"name": "alt3/cakebox-console",
	"description": "The CakePHP 3.x sources powering the Cakebox development environment",
	"homepage": "https://github.com/alt3/cakebox-console",
	"type": "project",
	"license": "MIT",
	"require": {
		"php": ">=5.6",
		"cakephp/bake": "~1.1",
		"cakephp/cakephp": "3.4.*",
		"cakephp/debug_kit": "~3.2",
		"cakephp/plugin-installer": "*",
		"mobiledetect/mobiledetectlib": "2.*",
		"monolog/monolog": "~1.0",
		"symfony/yaml": "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests",
			"Cake\\Test\\Fixture\\": "./vendor/cakephp/cakephp/tests/Fixture"
		}
	},
	"scripts": {
		"post-install-cmd": "App\\Console\\Installer::postInstall",
		"post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
	},
	"prefer-stable": true
}
